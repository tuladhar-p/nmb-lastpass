{
  "manifest_version": 2,
  "name": "NMB LastPass",
  "version": "2.1.1",
  "content_scripts": [
  {
    "matches": [
      "https://www.nmbbank.net/*"
    ],
    "js": ["content.js"]
  }
],
  "browser_action": {
  "default_icon": "favicon.png"
},
"background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}

